{
    "name": "sif-language",
    "displayName": "sif-language",
    "description": "sif language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.69.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "sif",
            "aliases": ["SIF", "sif"],
            "extensions": [".sif"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "sif",
            "scopeName": "source.sif",
            "path": "./syntaxes/sif.tmLanguage.json"
        }]
    }
}